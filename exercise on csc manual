"""a function that prints the factors of a number when entered"""
def factors():
    for x in range(1,100):
        if num%x==0:
            print (x)
num=input('enter the number')
number=int(num)

factors()

#a function that multiplies all factors of a number
def mult_factors(value):
    total=1
    for x in range(1,value+1):
        if value%x==0:
            total*=x
    return total   
                                        
value=input('enter the number')
values=int(value)

print(mult_factors(values))

#using a class to print the factors of a number and its product
class factor():
    def __init__(self,number):
        self.number=number
    def factors(self):
        facts=[]
        for each_num in range(1,self.number+1):
            if self.number%each_num==0:
                facts.append(each_num)
        return facts
    def factor_multiplier(self):
        multiplier=1
        for each_num in range(1,self.number+1):
            if self.number%each_num==0:
                multiplier*=each_num
        return multiplier
    
numb=input('enter the number please!')
numbs=int(numb)
task=factor(numbs)
print(task.factors())
print(task.factor_multiplier())         

#OR
def primenumbers(number):
    for x in numbers:
        if x%2==1:
            print(x)
numbers=range(1,25)
number=numbers
(primenumbers(number))            

        
    

